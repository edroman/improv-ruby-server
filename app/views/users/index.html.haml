%h1 Listing users
%table
  %tr
    %th First name
    %th Email
    %th Phone
    %th Password
    %th
    %th
    %th
  - @users.each do |user|
    %tr
      %td= user.first_name
      %td= user.email
      %td= user.phone
      %td= user.password
      %td= link_to 'Show', user
      %td= link_to 'Edit', edit_user_path(user)
      %td= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete
%br/
= link_to 'New User', new_user_path